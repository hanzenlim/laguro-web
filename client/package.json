    "start": "env-cmd ../config/security.env.local react-app-rewired start --max-old-space-size=16000",
    "build": "react-app-rewired build",
    "build-stage-assets": "PUBLIC_URL=https://s3.amazonaws.com/laguro-stage-assets/$(date '+%m-%d-%Y:')$(git log --pretty=format:'%h' -n 1) env-cmd /home/runner/security.env.stage npm run build",
    "deploy-stage-assets": "s3-deploy './build/**' --cwd './build/' --region us-east-1 --bucket laguro-stage-assets --gzip js,css,jpg,png,svg,map --cache 2503982 --filePrefix $(date '+%m-%d-%Y:')$(git log --pretty=format:'%h' -n 1)",
    "build-prod-assets": "PUBLIC_URL=https://d3smeczt2xieia.cloudfront.net/$(date '+%m-%d-%Y:')$(git log --pretty=format:'%h' -n 1) env-cmd /home/runner/security.env.prod npm run build client",
