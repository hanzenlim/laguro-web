{
	"name": "swapop",
	"version": "1.0.0",
	"description": "Airbnb for dental practices",
	"main": "server.js",
	"engines": {
		"node": "8.3.0",
		"npm": "5.3.0"
	},
	"scripts": {
		"start": "node server.js",
		"server": "nodemon server.js --exec babel-node --presets es2015,stage-2",
		"client": "npm run start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
	},
	"author": "currymich",
	"license": "ISC",
	"dependencies": {
		"apollo-fetch": "^0.7.0",
		"body-parser": "^1.18.2",
		"concurrently": "^3.5.1",
		"cookie-session": "^1.3.1",
		"cors": "^2.8.4",
		"express": "^4.15.4",
		"moment": "^2.20.1",
		"mongoose": "^4.11.8",
		"morgan": "^1.9.0",
		"passport": "^0.3.2",
		"passport-google-oauth20": "^1.0.0",
		"react-places-autocomplete": "^7.1.1",
		"save": "^2.3.2"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"concurrently": "^3.5.1",
		"nodemon": "^1.17.4",
		"webpack": "^4.6.0",
		"webpack-cli": "^2.0.15"
	}
}
