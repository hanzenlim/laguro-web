{
	"name": "swapop",
	"version": "1.0.0",
	"description": "Airbnb for dental practices",
	"main": "server.js",
	"engines": {
		"node": "8.3.0",
		"npm": "5.3.0"
	},
	"scripts": {
		"eslint:fix": "eslint client/src server.js models routes services --fix",
		"precommit": "npm run eslint:fix",
		"serverLocal": "nodemon serverLocal.js --exec babel-node --presets es2015,stage-2",
		"client": "npm run start --prefix client",
		"dev": "concurrently \"npm run serverLocal\" \"npm run client\"",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
	},
