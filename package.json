		"express": "^4.15.4",
		"moment": "^2.20.1",
		"mongoose": "^4.11.8",
		"morgan": "^1.9.0",
		"passport": "^0.3.2",
		"passport-google-oauth20": "^1.0.0",
		"save": "^2.3.2",
		"serverless": "^1.27.2",
		"serverless-http": "^1.5.5"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.3",
